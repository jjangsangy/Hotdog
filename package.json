{
  "name": "hotdog-imagenet",
  "version": "0.1.0",
  "description": "Hotdog Not Hotdog app running in the browser with tensorflow.js using a pretrained mobilenet model",
  "main": "app/app.js",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "0.6.1",
    "@tensorflow/tfjs-converter": "^0.1.0",
    "npm": "^5.8.0"
  },
  "scripts": {
    "watch": "NODE_ENV=development parcel --no-hmr --open app/index.html ",
    "build": "NODE_ENV=production parcel build ./app/index.html --public-url ./app"
  },
  "devDependencies": {
    "babel-minify": "^0.3.0",
    "babel-plugin-transform-runtime": "~6.23.0",
    "babel-polyfill": "~6.26.0",
    "babel-preset-env": "~1.6.1",
    "clang-format": "~1.2.2",
    "parcel-bundler": "~1.6.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 3 versions",
              "ie >= 9",
              "ios >= 8",
              "android >= 4.2"
            ]
          },
          "useBuiltIns": false
        }
      ]
    ],
    "plugins": [
      "transform-runtime"
    ]
  }
}